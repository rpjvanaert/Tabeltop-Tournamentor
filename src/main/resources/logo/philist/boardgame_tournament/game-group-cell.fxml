<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx"
      spacing="3"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="logo.philist.boardgame_tournament.controller.round.GameGroupCellController"
      VBox.vgrow="ALWAYS"
        >

    <HBox spacing="5" style="-fx-background-color: #0a255c; -fx-padding: 5; -fx-background-radius: 5;">
        <Label fx:id="groupNameLabel" textFill="white"/>
        <Label fx:id="playerCountLabel" textFill="white"/>
    </HBox>

    <ListView fx:id="playersListView"
    />

</VBox>
